{
  "rewrites": [
    {
      "source": "/",
      "destination": "/ai_welcome.html"
    },
    {
      "source": "/caonguyenthanhan(/|$)",
      "destination": "/personal_info.html"
    },
    {
      "source": "/caonguyenthanhan/pfofile(/|$)",
      "destination": "/profile_portfolio.html"
    },
    {
      "source": "/(genai/)?prompts(/|$)",
      "destination": "/image_prompt_library.html"
    },
    {
      "source": "/nlp(/|$)",
      "destination": "/nlp.html"
    },
    {
      "source": "/genai(/|$)",
      "destination": "/genai.html"
    },
    {
      "source": "/extensions(/|$)",
      "destination": "/extensions.html"
    }
  ]
}
